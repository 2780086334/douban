<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Servlers</servlet-name>
    <servlet-class>com.bdqn.servlet.Servlers</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>HomeServlers</servlet-name>
    <servlet-class>com.bdqn.servlet.HomeServlers</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Dianying2</servlet-name>
    <servlet-class>com.bdqn.servlet.Dianying2</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Dengru</servlet-name>
    <servlet-class>com.bdqn.servlet.Dengru</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>Servlers</servlet-name>
    <url-pattern>/servlet/Servlers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HomeServlers</servlet-name>
    <url-pattern>/servlet/HomeServlers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Dianying2</servlet-name>
    <url-pattern>/servlet/Dianying2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Dengru</servlet-name>
    <url-pattern>/servlet/Dengru</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
